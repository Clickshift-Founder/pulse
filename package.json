{
  "name": "pulse-agentic-wallet-os",
  "version": "2.0.0",
  "description": "Multi-Agent Agentic Wallet OS on Solana - Heartbeat Architecture, Thought Stream, Jupiter Swaps",
  "main": "dist/api/server.js",
"scripts": {
    "build": "tsc && npm run copy-static",
    "copy-static": "mkdir -p dist/dashboard/public && cp -r src/dashboard/public/* dist/dashboard/public/",
